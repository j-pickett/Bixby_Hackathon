action (SubmitSpot) {
  description (An action that takes a required Spot as an input)
  type (Calculation)
  collect {
    input (spotName) {
      type (SpotName)
      min (Required) max (One)
    }
    input (email) {
      type(Email)
      min (Required) max(One)
    }
    input (comments) {
      type (Comments)
      min (Required) max (One)
    }
    input (isSigningUp) {
      type (IsSigningUp)
      min (Required) max (One)
    }
  }
  output (Spot)
}
