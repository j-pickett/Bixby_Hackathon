endpoints {

  action-endpoints {
    action-endpoint (SubmitSpot) {
      accepted-inputs (spotName, category, description, isSigningUp)
      local-endpoint (submitSpot.js)
    }
    action-endpoint (CommitSpot) {
      accepted-inputs (spotName, category, description, isSigningUp)
      local-endpoint (commitSpot.js)
    }
  }
}
